<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment2">
    <fragment
        android:id="@+id/homeFragment2"
        android:name="com.example.sitask4.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment2_to_userFragment2"
            app:destination="@id/userFragment2" />
    </fragment>
    <fragment
        android:id="@+id/userFragment2"
        android:name="com.example.sitask4.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" >
        <action
            android:id="@+id/action_userFragment2_to_submissionFragment2"
            app:destination="@id/submissionFragment2" />
        <action
            android:id="@+id/action_userFragment2_to_solvedFragment2"
            app:destination="@id/solvedFragment2" />
        <action
            android:id="@+id/action_userFragment2_to_badgeFragment2"
            app:destination="@id/badgeFragment2" />
        <action
            android:id="@+id/action_userFragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/submissionFragment2"
        android:name="com.example.sitask4.SubmissionFragment"
        android:label="fragment_submission"
        tools:layout="@layout/fragment_submission" >
        <action
            android:id="@+id/action_submissionFragment2_to_solvedFragment2"
            app:destination="@id/solvedFragment2" />
        <action
            android:id="@+id/action_submissionFragment2_to_badgeFragment2"
            app:destination="@id/badgeFragment2" />
        <action
            android:id="@+id/action_submissionFragment2_to_userFragment2"
            app:destination="@id/userFragment2" />
        <action
            android:id="@+id/action_submissionFragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/solvedFragment2"
        android:name="com.example.sitask4.SolvedFragment"
        android:label="fragment_solved"
        tools:layout="@layout/fragment_solved" >
        <action
            android:id="@+id/action_solvedFragment2_to_userFragment2"
            app:destination="@id/userFragment2" />
        <action
            android:id="@+id/action_solvedFragment2_to_submissionFragment2"
            app:destination="@id/submissionFragment2" />
        <action
            android:id="@+id/action_solvedFragment2_to_badgeFragment2"
            app:destination="@id/badgeFragment2" />
        <action
            android:id="@+id/action_solvedFragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/badgeFragment2"
        android:name="com.example.sitask4.BadgeFragment"
        android:label="fragment_badge"
        tools:layout="@layout/fragment_badge" >
        <action
            android:id="@+id/action_badgeFragment2_to_homeFragment2"
            app:destination="@id/homeFragment2" />
        <action
            android:id="@+id/action_badgeFragment2_to_userFragment2"
            app:destination="@id/userFragment2" />
        <action
            android:id="@+id/action_badgeFragment2_to_submissionFragment2"
            app:destination="@id/submissionFragment2" />
        <action
            android:id="@+id/action_badgeFragment2_to_solvedFragment2"
            app:destination="@id/solvedFragment2" />
    </fragment>
</navigation>